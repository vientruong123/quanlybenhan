                            <a class="nav-link" href="~/BacSi/ThongTin/@userAdmin.MaBacSi">Thông Tin Cá Nhân</a>
                        </li>
                        <li class="nav-item">
                            <a class="btn btn-primary ml-lg-3" href="~/Home/DangXuat">Đăng Xuất</a>
                        </li>
                    </ul>
                </div> <!-- .navbar-collapse -->
            </div> <!-- .container -->
        </nav>
    </header>
    @RenderBody()
